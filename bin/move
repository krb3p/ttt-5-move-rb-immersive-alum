#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Welcome to Tic Tac Toe!"
puts "What move do you want to make?"
position = gets.strip
updated_position = input_to_index(position)
move(board, updated_position)
